{
  "nodes": [
    {
      "id": "Webhook",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [200, 200],
      "parameters": {
        "path": "requestFormHandy",
        "options": {},
        "responseCode": 200,
        "responseMode": "onReceived",
        "responseData": "Thanks for your request! We'll be in touch soon."
      }
    },
    {
      "id": "Set",
      "name": "Format Fields",
      "type": "n8n-nodes-base.set",
      "typeVersion": 1,
      "position": [400, 200],
      "parameters": {
        "fields": {
          "Name": "={{$json[\"name\"]}}",
          "Email": "={{$json[\"email\"]}}",
          "Phone": "={{$json[\"phone\"]}}",
          "Service": "={{$json[\"service\"]}}",
          "Preferred Date": "={{$json[\"date\"]}}",
          "Notes": "={{$json[\"message\"]}}"
        }
      }
    },
    {
      "id": "Email",
      "name": "Send Email",
      "type": "n8n-nodes-base.emailSend",
      "typeVersion": 1,
      "position": [600, 200],
      "parameters": {
        "fromEmail": "you@yourdomain.com",
        "toEmail": "you@yourdomain.com",
        "subject": "New HandyCrew4You Booking Request â€“ {{$json[\"Service\"]}}",
        "text": "New service request received:\n\nName: {{$json[\"Name\"]}}\nEmail: {{$json[\"Email\"]}}\nPhone: {{$json[\"Phone\"]}}\nService: {{$json[\"Service\"]}}\nPreferred Date: {{$json[\"Preferred Date\"]}}\nNotes:\n{{$json[\"Notes\"]}}"
      },
      "credentials": {
        "smtp": {
          "id": "YOUR_SMTP_CREDENTIAL_ID"
        }
      }
    }
  ],
  "connections": {
    "Webhook Trigger": {
      "main": [["Format Fields"]]
    },
    "Format Fields": {
      "main": [["Send Email"]]
    }
  }
}
